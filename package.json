{
  "name": "gold",
  "version": "0.0.0",
  "description": "Management and monitoring UI for apps built with Lighter",
  "main": "gold.js",
  "scripts": {
    "test": "mocha",
    "retest": "mocha --watch",
    "cover": "istanbul cover _mocha",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zerious/gold"
  },
  "keywords": [
    "lighter",
    "app",
    "management",
    "monitoring",
    "web",
    "ui",
    "user",
    "interface",
    "development",
    "environment"
  ],
