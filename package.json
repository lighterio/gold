{
  "name": "gold",
  "version": "0.0.0",
  "description": "Management and monitoring UI for apps built with Lighter",
  "main": "gold.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lighterio/gold"
  },
  "keywords": [
    "lighter",
    "app",
    "management",
    "monitoring",
    "web",
    "ui",
    "user",
    "interface",
    "development",
    "environment"
  ],
  "author": "Sam Eubank <sameubank@gmail.com>",
  "bugs": {
    "url": "https://github.com/lighterio/gold/issues"
  },
  "homepage": "https://github.com/lighterio/gold",
  "contributors": [
    "Sam Eubank <sameubank@gmail.com>"
  ],
  "engines": [
    "node >= 0.2.6"
  ],
  "scripts": {
    "test": "./node_modules/exam/exam.js",
    "retest": "./node_modules/exam/exam.js --watch",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/exam/exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "dependencies": {
    "shellify": "0.0.2"
  },
  "devDependencies": {
    "zeriousify": "0.1.9",
    "exam": "0.0.5",
    "istanbul": "0.2.11"
  },
